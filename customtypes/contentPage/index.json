{
  "id": "contentPage",
  "label": "Content Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Core": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "",
          "useAsTitle": true
        }
      },
      "uid": {
        "config": {
          "label": "Slug"
        },
        "type": "UID"
      }
    },
    "Metadata": {
      "metaDescription": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "metaImage": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    },
    "Article": {
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image_block": {
              "type": "SharedSlice"
            },
            "text_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
