{
  "id": "projectIndex",
  "label": "[PAGE] Project Index",
  "format": "page",
  "repeatable": false,
  "status": true,
  "json": {
    "Core": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "",
          "useAsTitle": true
        }
      }
    },
    "Metadata": {
      "metaDescription": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "metaImage": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    },
    "Intro": {
      "intro": {
        "type": "StructuredText",
        "config": {
          "label": "Intro",
          "placeholder": "Introduction text that appears above the projects list",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      }
    }
  }
}
